public class Main {
    public static void main(String[] args) {
        Shape circle = new Circle(5);  // Radio del círculo
        Shape square = new Square(4);  // Lado del cuadrado

        System.out.println("El área del círculo es: " + circle.calculateArea());
        System.out.println("El área del cuadrado es: " + square.calculateArea());
    }
}
